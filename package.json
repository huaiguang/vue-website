{
  "name": "vue-website",
  "version": "1.0.0",
  "description": "large website template based on vue",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "clean:all": "rm -rf ./node_modules && rm -rf ./dist && rm package-lock.json",
    "eslint": "eslint ./src/",
    "eslint:fix": "eslint ./src/ --fix",
    "test": "webpack --config ./config/webpack.conf.test",
    "dev": "cross-env NODE_ENV=development node ./config/dev-server",
    "dev:server": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.conf.test",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.conf.prod",
    "release": "cross-env NODE_ENV=production node ./scripts/build.js"
  },
  "author": "Carlos",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/huaiguang/vue-website.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.2",
    "opn": "^6.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "element-ui": "^2.13.0",
    "fullpage.js": "^3.0.4",
    "jquery": "^3.3.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.5.16",
    "vue-router": "^3.1.6"
  }
}
