{
  "name": "webpack-restarter",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "clean:all": "rm -rf ./node_modules && rm -rf ./dist",
    "test": "webpack --config ./config/webpack.config.test",
    "devServer": "webpack-dev-server --inline --config ./config/webpack.config.test",
    "dev": "cross-env NODE_ENV=dev node ./config/dev-server",
    "build": "cross-env NODE_ENV=prod webpack --config ./config/webpack.config.prod",
    "release": "cross-env NODE_ENV=prod node ./scripts/build.js"
  },
  "keywords": [
    "webpack",
    "postcss",
    "eslint",
    "es6",
    "vue",
    "react"
  ],
  "author": "chenjiaxin",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:huaiguang/webpack-restarter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "normalize.css": "^7.0.0"
  }
}
